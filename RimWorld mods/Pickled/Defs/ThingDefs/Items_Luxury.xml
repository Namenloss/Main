<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <ThingDef ParentName="MealBase">
    <defName>Vodka</defName>
    <label>vodka</label>
    <description>A potent spirit derived from potatoes. Its main effect is intoxication. Excessive consumption can lead to alcohol addiction.</description>
    <graphicData>
      <texPath>Things/Item/Drug/Vodka</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <equippedAngleOffset>-150</equippedAngleOffset>
    <rotatable>false</rotatable>
    <stackLimit>25</stackLimit>
    <statBases>
      <DeteriorationRate>0</DeteriorationRate>
      <MarketValue>13</MarketValue>
      <MeleeWeapon_DamageAmount>7</MeleeWeapon_DamageAmount>
      <MeleeWeapon_Cooldown>1.8</MeleeWeapon_Cooldown>
    </statBases>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
      <foodType>Fluid, Processed, NutritionalDrug</foodType>
      <nutrition>0.03</nutrition>
      <joy>0.18</joy>
      <joyKind>Chemical</joyKind>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
      <soundEat>Ingest_Beer</soundEat>
      <isPleasureDrug>true</isPleasureDrug>
      <hediffGivers>
        <li>
          <chance>1</chance>
          <hediffDef>Alcohol</hediffDef>
        </li>
      </hediffGivers>
    </ingestible>
    <equipmentType>Primary</equipmentType>
    <techLevel>Neolithic</techLevel>
    <comps>
      <li>
        <compClass>CompEquippable</compClass>
      </li>
    </comps>
    <verbs>
      <li>
        <verbClass>Verb_MeleeAttack</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <meleeDamageDef>Blunt</meleeDamageDef>
      </li>
    </verbs>
  </ThingDef>

